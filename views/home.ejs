    <style>
        * {
            font-family: 'Montserrat', sans-serif;
            color: white;
            padding: 0px;
            margin: 0px;
            scroll-behavior: smooth;

        }

        body {
            overflow-x: hidden;
        }

        .part-1 {
            height: 100vh;
            background: black;
        }

        .part-2 {
            height: 100vh;
            background-color: #0084FF;
        }

        .part-3 {
            height: 100vh;
            background-color: #E73006;
        }

        .part-4 {
            height: 100vh;
            background-color: #9b59b6;
        }

        .navb {
            text-align: center;
            /* width: 100vh; */
            /* height: min-content; */
            /* font-size: 2rem; */
            height: 100px;
            padding-top: 25px;
            padding-bottom: 7%;
        }

        .navb h1 a s {
            font-size: 4.2rem;
            color: #C2FE59;
            text-align: center;
            font-weight: bolder;
        }


        .navb h1 a:hover {
            display: inline-block;
            color: #6bdcf9;
            transform: scale(1.1);
            transition: 150ms ease-in-out;
            cursor: pointer;
            color: #f96b95;
        }

        .details {
            text-align: center;
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
        }

        .details h1 {
            font-size: 4em;
            font-weight: 600;
            padding-bottom: 30px;
        }

        .details span h1 {
            font-size: 2em;
        }

        .details img {
            height: 400px;
            width: 400px;
            border-radius: 50%;
            border: 3px solid #C2FE59;
        }

        .typer-js {
            font-size: 2.0rem;
            font-weight: bold;
            margin-top: 50px;
            border: 2px solid #C2FE59;
            padding: 10px;
            border-radius: 1px;
        }

        .social {
            display: flex;
            /* width: 300px; for mobile screen */
            margin-top: 60px;
            width: 50%;
            justify-content: space-between;
            align-items: center;
        }

        .social a i:hover {
            color: #C2FE59;
            transform: scale(1.2);
            transition: 150ms ease-in-out;
        }

        .deschead {
            text-align: center;
            padding-top: 2%;
            /* font-size: 2rem; */
            height: 17vh;
        }

        .deschead h1 {
            font-size: 4.2rem;
            font-weight: bolder;
        }

        .container {
            display: flex;
            justify-content: space-evenly;
            flex-direction: row;
            align-items: center;
            /* height: 90%; */
            /* flex-direction: column; for mobile */
        }

        .desc {
            width: 25%;
            text-align: center;
            line-height: 2;
            font-size: 1.2rem;
        }

        .desc h1 {
            font-size: 2.0rem;
            padding-bottom: 30px;
            font-weight: 600;
        }

        span .abt-img img {
            height: 640px;
            width: 480px;
            border: 3px dashed white;
            padding: 10px;
        }

        .proj-head {
            text-align: center;
            height: 3vh;
        }

        .proj-head h1 {
            font-size: 3.5rem;
            font-weight: bolder;
            padding-top: 2%;
        }

        .projects {
            text-align: center;
            /* width: 60%; */
            /* margin-right: 2%; */
            width: 80%;
            margin-left: auto;
            margin-right: auto;

        }

        .slick-slide {
            margin-left: 2.8%;
            margin-right: 2.9%;
            margin-top: 9rem;
            margin-bottom: 2rem;
        }

        .slick-slide img {
            display: block;
            width: 350px;
            overflow: none;
            height: 460px;
        }

        .main-card {
            background-color: #2F4F4F;
        }

        .part4-main {
            display: flex;
            align-items: center;
            flex-direction: column;
        }

        .part4-main h1 {
            margin-top: 2%;
            margin-bottom: 1%;
            font-weight: 500;
        }

        .part4-main p {
            margin-top: 2%;
            margin-bottom: 2%;
            width: 40%;
            text-align: center;
            font-size: 1.3rem;
        }

        .c-head {
            text-align: center;
            padding-top: 2%;
        }

        .c-head h1 {
            font-size: 4rem;
            font-weight: bolder;
        }

        .msgr-btn {
            text-align: center;
            background-color: white;
            /* padding-top: 8px; */
            /* padding-bottom: 8px; */
            padding-left: 10px;
            padding-right: 10px;
            color: #0099FF;
            display: flex;
            align-items: center;
            justify-content: space-around;
            width: 270px;
            height: 55px;
            border-radius: 4px;
            font-weight: 600;
            font-size: 1.15rem;

        }

        .msgr-btn:hover {
            cursor: pointer;
            transform: scale(1.1);
            transition: 300ms ease-in-out;
        }

        .msgr-btn i {
            color: #0099FF;
        }

        .qn-head {
            text-align: center;
        }

        .qn-head h1 {
            font-size: 2.8rem;
            font-weight: 600;
        }

        .qn-head p {
            width: 50%;
            font-size: 1.5rem;
        }

        /* 
        input {
            margin-bottom: 10px;
            color: black;
            background: #9b59b6;
        } */


        input[type=text] {
            margin-bottom: 10px;
            /* color: black; */
            background: #9b59b6;
        }

        textarea {
            /* color: white; */
            border-radius: 5px;
            background: #9b59b6;
            width: 100%;
        }

        .formd button {
            color: white;
            border: 2px solid #0084FF;
            font-weight: 700;
            width: 100%;
            margin-top: 8px;
        }

        .formd input::placeholder {
            color: white;
        }

        .formd {
            width: 40%;
        }

        .footer h2 a {
            cursor: pointer;
            text-decoration: none;

        }

        .footer {
            margin-top: 15px;
            text-align: center;
            /* padding-top: 30px; */
            /* padding-bottom: 30px; */
        }

        .footer h2 {
            font-size: 20px;
        }

        /* .footer a:hover{
            display: inline-block;

        } */


        /* optimization for mobile */
        @media screen and (max-width: 450px) {
            .details img {
                height: 350px;
                width: 350px;
            }

            .navb {
                padding: 0px;
                padding-top: 10px;
            }

            .details h1 {
                font-size: 2.5rem;
            }

            .typer-js {
                font-size: 1.6rem;
            }

            .social {
                width: 85%;
            }

            .container {
                flex-direction: column;
            }

            .abt-img {
                margin-top: 30px;
            }

            span .abt-img img {
                height: 466px;
                width: 350px;
            }

            .desc {
                width: 90%;
                font-size: 1rem;
            }

            .desc h1 {
                font-size: 1.2rem;
                padding: 10px;
            }

            .deschead {
                font-size: 1.5rem;
                height: 8vh;
            }

            .card {
                height: 700px;
            }

            .slick-slide {
                /* margin-left: 2.8%;
            margin-right: 2.9%; */

                margin-left: 31px;
                margin-right: 0px;
                margin-top: 4rem;
            }

            textarea {
                /* width: 370px; */
                height: 100px;
            }

            .part4-main h1 {
                font-size: 2rem;
                font-weight: 600;
            }

            .part4-main p {
                width: 85%;
            }

            .msgr-btn {
                margin-top: 20px;
                margin-bottom: 20px;
            }

            .formd {
                padding-top: 20px;
                width: 90%;
            }

            .footer {
                text-align: center;
                margin-top: 30px;
            }

            .footer h2 {
                font-size: 20px;
            }
        }
